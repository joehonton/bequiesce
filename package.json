{
  "name": "Bequiesce",
  "version": "1.0.0",
  "description": "Because 99.999 Doesn't Just Happen",
  "author": "Joe Honton",
  "private": true,
  "license": "CC-BY-NC-ND-4.0",
  "main": "main.js",
  "dependencies": {
  },
  "devDependencies": {
    "babel": "^5.8.21"
  },
  "scripts": {
    "buildlib":    	"npm run compilelib && npm run runlib",
    "compilelib":  	"babel --out-dir=lib --source-maps --extensions=\".js\" src",
    "runlib":      	"node examples/05-spherical-coordinates/test-suite.js",
    
    "tuesday": 		"npm run eximA && npm run eximB && npm run eximC && npm run use-es5",
    "eximA":       	"bash ../exim/exim ./src/main.js ./es5",
    "eximB":       	"bash ../exim/exim ./examples/05-spherical-coordinates/test/case/sphericoords.1.test.js ./examples/05-spherical-coordinates/test/es5",
    "eximC":       	"bash ../exim/exim ./examples/05-spherical-coordinates/test/case/sphericoords.2.test.js ./examples/05-spherical-coordinates/test/es5",
    "eximD":       	"bash ../exim/exim ./examples/05-spherical-coordinates/test/case/tiny.test.js ./examples/05-spherical-coordinates/test/es5",
    "use-es5":     	"bash ../bequiesce/bequiesce ./examples/05-spherical-coordinates/test/es5",
    
    "build05":     	"npm run compilelib && npm run exim05 && npm run bequiesce05",
    "exim05":      	"bash ../exim/exim ./src/main.js ./es5",
    "bequiesce05": 	"./bequiesce ./examples/05-spherical-coordinates/test/exim/05-test-harness.js"
  },
  "#": "These are babel command line options",
  "babel": {
    "modules": "common",
    "sourceMaps": true
  }
}
